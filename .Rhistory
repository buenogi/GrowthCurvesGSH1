scale_x_discrete(labels = c("0h", "24h", "48h", "72h", "96h")) +
facet_wrap(~pop, nrow = 2)+
theme_bw()
GrowthC_plot_Clone
ggsave("Figuras/02_GrowthC_plot_Clone.png")
